{
  "name": "moto-e4-plus",
  "version": "0.0.1",
  "description": "Layout Motorola E4 Plus",
  "main": "index.js",
  "scripts": {
    "test": "jest --env=jsdom",
    "dev": "webpack --config ./webpack.config.js -p --progress --watch",
    "prod": "webpack --config ./webpack.config.js -p --progress"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "moduleNameMapper": {
      "\\.(css|less|sass)$": "<rootDir>/src/__mocks__/styleMock.js",
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kmiloarguello/moto-e4-plus.git"
  },
  "keywords": [
    "Jumbo",
    "Cencosud",
    "Layout",
    "Cssgrid",
    "Webpack",
    "React"
  ],
  "author": "@kmiloarguello",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kmiloarguello/moto-e4-plus/issues"
  },
  "homepage": "https://github.com/kmiloarguello/moto-e4-plus#readme",
  "devDependencies": {
    "animate.css": "^3.6.1",
    "autoprefixer": "^8.6.5",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.4.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^1.0.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.4.1",
    "jest-mock": "^23.2.0",
    "path": "^0.12.7",
    "postcss": "^6.0.23",
    "postcss-cli": "^5.0.1",
    "postcss-cssnext": "^3.1.0",
    "postcss-font-magician": "^2.2.1",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.6",
    "preload-webpack-plugin": "^2.3.0",
    "react-testing-library": "^4.1.2",
    "regenerator-runtime": "^0.12.0",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.16.0",
    "webpack-cli": "^3.0.8"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "wowjs": "^1.1.3"
  }
}
